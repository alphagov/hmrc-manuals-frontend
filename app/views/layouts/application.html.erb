</head>
<body>
  <div id="global-breadcrumb" class="header-context"></div>
  <div id="wrapper">
    <main id="content" role="main" class="hmrc-manuals-frontend-content outer-block">
      <div id="manuals-frontend" class='inner-block'>
        <%= yield %>
        <div id="report-a-problem"></div>
      </div>
    </main>
  </div>
